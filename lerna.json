{
  "version": "2.0.0-alpha.0",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "configure/*",
    "packages/*"
  ],
  "command": {
    "bootstrap": {
      "npmClientArgs": ["--frozen-lockfile"]
    },
    "publish": {
      "canary": true,
      "preid": "alpha",
      "access": "public"
    }
  }
}
